"vertexlitgeneric"
{
	"$basetexture" "models/weapons/w_hookshot_scout/v_sgrapple"

	"$phong" "1"
	"$phongexponent" "0.1"
	"$phongboost" "0.2"	
	"$lightwarptexture" "models\lightwarps\weapon_lightwarp"
	"$phongfresnelranges"	"[.25 .5 1]"
	"$basemapalphaphongmask" "1"

	"$rimlight" "1"
	"$rimlightexponent" "10"	
	"$rimlightboost" "1"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"

	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
