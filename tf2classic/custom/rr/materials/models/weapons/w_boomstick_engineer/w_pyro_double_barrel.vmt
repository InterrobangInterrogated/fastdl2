"SDK_VertexLitGeneric"
{
	"$basetexture"	"models/weapons/w_boomstick_engineer/w_pyro_double_barrel"
	"$bumpmap"	"models\weapons\v_pyro_double_barrel\v_pyro_double_barrel_n"
	"$surfaceprop" 	"Metal"
	
	"$phong" "1"
	"$phongexponent" "25"
	"$phongboost" "1"	
	"$halflambert"	"1"
	"$phongfresnelranges"	"[.7 15 20]"
	//"$bumpmap" "Models/effects/flat_normal"
//	"$lightwarptexture" "models\lightwarps_bucket\weapon_lightwarp"


	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps/cubemap_sheen001"
	"$sheenmapmask" 		"Effects/AnimatedSheen/animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"


	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
