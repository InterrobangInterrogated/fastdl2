"VertexLitGeneric"
{
	"$baseTexture" "\models\weapons\w_stabby\proknife"
	"$lightwarptexture" "models\weapons\w_stabby\c_ambassador_lightwarp"

	$BASEMAPALPHAPHONGMASK 1

	"$normalmapalphaenvmapmask" 1
	"$envmap" "env_cubemap"
	"$envmaptint" "[.2 .2 .2]"

	"$phong" "1"
	"$phongexponent" "20"
	"$phongboost" "15"
//	"$lightwarptexture" "models\lightwarps_bucket\weapon_lightwarp"

	"$phongfresnelranges"	"[.25 .5 1]"

	"$rimlight" "1"
	"$rimlightexponent" "4"	
	"$rimlightboost" "2"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"Proxies"
	{
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
