"VertexLitGeneric"
{
	"$basetexture" "models/weapons/w_guitar/w_guitar"
	"$bumpmap" "models/weapons/w_guitar/w_guitar_normal"
	//"$envmap" "env_cubemap"
	//"$normalmapalphaenvmapmask" 1
	//"$basemapalphaphongmask" 1

	
	"$phong" "1"
	"$phongboost" "2"	
	"$lightwarptexture" "models/lightwarps/weapon_lightwarp"	
	"$phongfresnelranges"	"[.3 1 8]"
	"$halflambert" "0"
	
	// Rim lighting parameters
	//"$rimlight" "0"												// To enable rim lighting (requires phong)
	//"$rimlightexponent" "4"										// Exponent for phong component of rim lighting	
	//"$rimlightboost" ".5"										// Boost for ambient cube component of rim lighting

	// Variables for the cloak effect
	"$cloakPassEnabled"     "1"

	// Variables for the burning effect
	"$detail"               "effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"          "5"
	"$detailblendfactor"    "0"
	"$detailblendmode"      "6"

	"$selfillumtint"	"[1 1 1]"
	"$selfillummask"	""

	// Variable for crit glow
	"$glowcolor" "1"

	// The order of the proxies is important!
	"Proxies"
	{
	// Proxies for the cloak effect
		"invis"
		{
		}
		// Proxies for crit glow
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
	}
}
