"VertexLitGeneric"
{
	"$basetexture"		"models/weapons/w_secondrate_sorcery/secondrate_sorcery_color"
	"$bumpmap"		"models/weapons/w_secondrate_sorcery/secondrate_sorcery_normal"
	"$lightwarptexture"		"models/player/pyro/pyro_lightwarp"
//	"$lightwarptexture"		"models/lightwarps_bucket/weapon_lightwarp"

	"$phong"			"1"
	"$phongexponent"		"25"
	"$phongboost"		"0.100000"
	"$phongfresnelranges"		"[.25 .5 1]"
	"$rimlight"		"1"
	"$rimlightexponent"		"4"
	"$rimlightboost"		"2"
	"$color2"		"{ 255 255 255 }"
	"$bumpmapalphaphongmask"		"1"
	"$cloakPassEnabled"		"1"
	"$detail"		"effects/tiledfire/fireLayeredSlowTiled512"
	"$detailscale"		"5"
	"$detailblendfactor"		"0"
	"$detailblendmode"		"6"

	"$glowcolor" "1"

	// Cloaking
	"$cloakPassEnabled" "1"
	"$sheenPassEnabled" "1"

	"$sheenmap" 		"cubemaps\cubemap_sheen001"
	"$sheenmapmask" 		"Effects\AnimatedSheen\animatedsheen0"
	"$sheenmaptint" 		"[ 1 1 1 ]"
	"$sheenmapmaskframe" 	"0"
	"$sheenindex" 		"0"

	"$yellow" "0"

	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"AnimatedTexture"
		{
			"animatedtexturevar"		"$detail"
			"animatedtextureframenumvar"		"$detailframe"
			"animatedtextureframerate"		"30"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
	}
}
