	///////////////////////////////////////////////////////////////////////////////
	//$BlendTintByBaseAlpha 1
	//$BlendTintColorOverBase 1.0

	//$colortint_base "[1 1 1]"
	//$colortint_tmp "[0 0 0]"
	//$color2 "{60 60 65}"

	$BaseMapAlphaPhongMask 1


	///////////////////////////////////////////////////////////////////////////////
	//					special effects.
	///////////////////////////////////////////////////////////////////////////////
	$yellow 0
	$glowcolor 1

	$cloakPassEnabled 1
	$sheenPassEnabled 1

	$sheenmap		"cubemaps\cubemap_sheen001"
	$sheenmapmask		"Effects\AnimatedSheen\animatedsheen0"
	$sheenmaptint		"[ 1 1 1 ]"
	$sheenmapmaskframe 	0
	$sheenindex 		0


	///////////////////////////////////////////////////////////////////////////////
	//					Proxies.
	///////////////////////////////////////////////////////////////////////////////
	"Proxies"
	{
		"AnimatedWeaponSheen"
		{
			"animatedtexturevar" 		"$sheenmapmask"
			"animatedtextureframenumvar" 	"$sheenmapmaskframe"
			"animatedtextureframerate" 		"40"
		}
		"invis"
		{
		}
		"ModelGlowColor"
		{
			"resultVar" "$glowcolor"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$selfillumtint"
		}
		"Equals"
		{
			"srcVar1"  "$glowcolor"
			"resultVar" "$color2"
		}
		"YellowLevel"
		{
			"resultVar" "$yellow"
		}
		"Multiply"
		{
			"srcVar1" "$color2"
			"srcVar2" "$yellow"
			"resultVar" "$color2"
		}
